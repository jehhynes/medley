{
  "version": "3.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "provider": "unpkg",
      "library": "@microsoft/signalr@latest",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js",
        "dist/browser/signalr.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@coreui/icons@2.1.0",
      "destination": "wwwroot/lib/coreui/icons/",
      "files": [
        "css/all.css",
        "css/all.min.css",
        "fonts/*"
      ]
    },
    {
      "provider": "cdnjs",
      "library": "bootstrap-icons@1.13.1",
      "destination": "wwwroot/lib/bootstrap-icons/",
      "files": [
        "font/fonts/bootstrap-icons.woff",
        "font/fonts/bootstrap-icons.woff2",
        "font/bootstrap-icons.css",
        "font/bootstrap-icons.min.css"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "bootstrap@5.3.0",
      "destination": "wwwroot/lib/bootstrap/",
      "files": [
        "dist/js/bootstrap.bundle.min.js",
        "dist/js/bootstrap.bundle.min.js.map"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "vue@3.4.21",
      "destination": "wwwroot/lib/vue/",
      "files": [
        "dist/vue.global.js",
        "dist/vue.global.prod.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "@alenaksu/json-viewer@2.1.0",
      "destination": "wwwroot/lib/json-viewer/",
      "files": [
        "dist/json-viewer.bundle.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "marked@12.0.0",
      "destination": "wwwroot/lib/marked/",
      "files": [
        "marked.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "bootbox@6.0.0",
      "destination": "wwwroot/lib/bootbox/",
      "files": [
        "dist/bootbox.all.min.js"
      ]
    },
    {
      "provider": "jsdelivr",
      "library": "chart.js@4.4.0",
      "destination": "wwwroot/lib/chart.js/",
      "files": [
        "dist/chart.umd.min.js"
      ]
    }
  ]
}