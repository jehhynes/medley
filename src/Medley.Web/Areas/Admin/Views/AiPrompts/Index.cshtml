@{
    ViewData["Title"] = "AI Prompts";
    ViewData["HasLeftSidebar"] = true;
    Layout = "_VueLayout";
}

@* Vue SFC: Vue/pages/AiPrompts.vue (compiled into app.js) *@
@* Entry point: wwwroot/js/pages/ai-prompts.js (mounts the SFC) *@
<div id="app"></div>

@section Scripts {
    <script src="~/dist/app.js" asp-append-version="true"></script>
    <script src="~/js/pages/ai-prompts.js" asp-append-version="true"></script>
}
